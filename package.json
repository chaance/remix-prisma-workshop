{
  "name": "remix-prisma-workshop",
  "version": "1.0.0",
  "description": "Learn the fundamentals of Remix with this workshop",
  "main": "index.js",
  "scripts": {
    "typecheck": "tsc -b .",
    "lint": "node ./scripts/run-in-all.js \"npm run lint\"",
    "build": "node ./scripts/build",
    "dev": "node dev",
    "diff": "node diff",
    "// setup": "npm install && node ./scripts/setup && run-p typecheck lint build",
    "setup": "npm install && node ./scripts/setup && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaance/remix-prisma-workshop.git"
  },
  "keywords": [
    "remix",
    "workshop"
  ],
  "author": "Chance Strickland <hi@chance.dev> (https://chance.dev/)",
  "contributors": [
    "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com/)"
  ],
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/chaance/remix-prisma-workshop/issues"
  },
  "homepage": "https://github.com/chaance/remix-prisma-workshop#readme",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "typescript": "^4.6.4"
  },
  "workspaces": [
    "exercise/*",
    "final/*"
  ]
}
